{
  "kind": "collectionType",
  "collectionName": "services",
  "info": {
    "singularName": "service",
    "pluralName": "services",
    "displayName": "Service"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "uid"
    },
    "name": {
      "type": "string"
    },
    "imageHero": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "images": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "description": {
      "type": "text"
    },
    "sessionDetails": {
      "type": "text"
    },
    "benefits": {
      "type": "json"
    },
    "sessionsAvailable": {
      "type": "string"
    },
    "price": {
      "type": "decimal"
    },
    "duration": {
      "type": "string"
    },
    "est_publie": {
      "type": "boolean"
    },
    "service": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::service.service",
      "inversedBy": "servicesSimilaires"
    },
    "servicesSimilaires": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::service.service",
      "mappedBy": "service"
    },
    "similar_description": {
      "type": "text"
    }
  }
}
